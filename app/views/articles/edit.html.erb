<div class ="d-flex flex-column align-items-center justify-content-center wrapper">
<%= form_with model: @article, local: true do |f| %>

<p>
    <h5><%= f.label :name %></h5>
    <%= f.text_field :name %>
</p>

<p>
    <h5><%= f.label :body %></h5>
    <%= f.text_field :body %>
</p>

<p>
    <%= f.submit %>
</p>

<%end %>

<%= link_to 'Back', article_path , class: 'backBtn' %>

</div>